<div class="attributes">
  <div class="attribute">
    <div class="field"> Emp Id </div>
    <div class="value"> <%= @intern.emp_id %> </div>
  </div>

  <div class="attribute">
    <div class="field"> DOB </div>
    <div class="value"> <%= @intern.dob %> </div>
  </div>

  <div class="attribute">
    <div class="field"> Gender </div>
    <div class="value"><%= @intern.gender.humanize %></div>
  </div>

  <div class="attribute">
    <div class="field"> Batch </div>
    <div class="value"> <%= @intern.batch.name %> </div>
  </div>
</div>