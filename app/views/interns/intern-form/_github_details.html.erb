<div class="panel">
  <div class="panel-heading"> Github Details </div>
  <div class="attributes">
    <%= form.fields_for :github do |github_fields| %>
      <div class="attribute">
        <div class="field"> Username </div>
        <div class="value"> <%= github_fields.text_field :username, placeholder: 'Github Username'  %> </div>
      </div>
    <% end %>
  </div>
</div>